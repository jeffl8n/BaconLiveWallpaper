<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.builtclean.android.livewallpapers.bacon.donate"
	android:versionName="1.3" android:versionCode="2100000003">
	
	<uses-sdk android:minSdkVersion="7" /><uses-feature android:name="android.software.live_wallpaper" android:required="true"></uses-feature>

	<application android:icon="@drawable/icon" android:label="@string/app_name" android:debuggable="false">


		<service android:name="BaconLiveWallpaper" android:label="@string/app_name"
			android:icon="@drawable/icon" android:permission="android.permission.BIND_WALLPAPER">
			<intent-filter>
				<action android:name="android.service.wallpaper.WallpaperService"></action>
			</intent-filter>
			<meta-data android:resource="@xml/wallpaper" android:name="android.service.wallpaper"></meta-data>
		</service>

		<activity android:label="@string/wallpaper_settings"
			android:name="BaconLiveWallpaperSettings" android:theme="@android:style/Theme.Light.WallpaperSettings"
			android:exported="true">
		</activity>

		<activity android:name="OpenActivity" android:icon="@drawable/icon"
			android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"></action>
				<category android:name="android.intent.category.INFO"></category>
			</intent-filter>
		</activity>
	</application>


</manifest> 